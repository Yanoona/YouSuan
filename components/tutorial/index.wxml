<i-panel title="文章查询">
  <view style="padding: 15px;">
    <i-swipeout i-class="i-swipeout-demo-item" operateWidth="{{180}}" unclosable="{{true}}" toggle="{{toggle}}">
      <view slot="content">
        <view class="i-seach">
          <i-input value="" placeholder="在此处输入需要查找的文章或作者（右滑搜索）" i-class='headSeach' />
        </view>
      </view>
      <view slot="button" class="i-swipeout-demo-button-group" style="background:#2db7f5;">
        <view class="i-swipeout-demo-button" style="width:180px" bindtap="actionsTap">
          搜索
        </view>
      </view>
    </i-swipeout>
  </view>
</i-panel>

